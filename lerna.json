{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "independent",
  "packages": ["packages/*"],
  "yes": true,
  "distTag": "previous",
  "preDistTag": "next",
  "command": {
    "version": {
      "preid": "next",
      "message": "chore(release): publish",
      "conventionalCommits": true,
      "contents": "package"
    }
  }
}
